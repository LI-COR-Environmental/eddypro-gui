QToolBar {
    background-image: url(:/icons/bgnormal);
    background-attachment: fixed;
    height: 43px;
    border-top: 1px solid black;
    border-bottom: 1px solid black;
    border-left: 0px solid black;
    border-right: 0px solid black;
    padding: 0px;
    margin: 0px;
    spacing: 0px;
}

QToolBar::handle {
    image: url(:/icons/handle);
    border-top: 1px solid black;
    border-bottom: 1px solid black;
}

QToolBar:first {
    border-left: 0px solid black;
    border-right: 1px solid black;
}

QToolBar:middle {
    border-left: 0px solid black;
    border-right: 1px solid black;
}

QToolBar:last {
    border-left: 1px solid black;
    border-right: 0px solid black;
}

QToolBar::separator {
    image: url(:/icons/sep);
    border-top: 1px solid black;
    border-bottom: 1px solid black;
}

QToolBar::separator:pressed {
    image: url(:/icons/sepsel);
    border-bottom: 1px solid black;
}

QToolButton {
    border: none;
    background: transparent;
}

QToolButton:pressed {
    background: transparent;
}

QToolButton:checked {
    background: transparent;
    background-image: url(:/icons/bgsel);
    border-top: 1px solid black;
    border-bottom: 1px solid black;
    border-right: 0px solid black;
}

QToolButton:checked:hover {
    background: transparent;
    background-image: url(:/icons/bgsel);
}

QToolButton:!checked:hover {
    background: qlineargradient(spread:pad, x1:0, y1:0.5, x2:1, y2:0.5,
                stop:0 rgba(235,230,235,0),
                stop:0.2 rgba(235,230,235,1),
                stop:0.8 rgba(235,230,235,1),
                stop:1 rgba(235,230,235,0));
}

