QTabWidget {
    background: transparent;
}

QTabWidget::pane {
    position: absolute;
    top: -10px;
    border: 0px;
    border-top: 1px solid qradialgradient(cx:0.5, cy:0.5, radius: 1,
                    fx:0.5, fy:0.5,
                    stop:0 #666666,
                    stop:0.5 rgba(255,255,255,0),
                    stop:1 rgba(255,255,255,0));
    padding: 0px 0px 3px 0px;
    margin-bottom: -10px;
    background: transparent;
}

QTabWidget::tab-bar {
    alignment: center;
}

QTabWidget[leftTabBar="true"]::tab-bar  {
    alignment: left;
}

/* necessary for Qt bug reason, in sync with the corrisponding properties
   in QTabBar::tab */
QTabBar {
    font: normal normal 12px "Open Sans";
}

QTabBar::tab {
    border: 1px solid #666666;
    padding: 2px 12px 3px 12px;
    margin-right: -1px;
    margin-left: -1px;
    font: normal normal 12px "Open Sans";
    color: #666666;
}

QTabBar::tab:first {
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
    padding: 2px 11px 3px 12px;
    margin-left: 0;
}

QTabBar::tab:last {
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    padding: 2px 12px 3px 11px;
    margin-right: 0;
}

QTabBar::tab:only-one {
    border-radius: 4px;
    padding: 2px 12px 3px 12px;
    margin: 0;
}

QTabBar::tab::disabled {
    color: black;
    dither-disable-text: 0;
    etch-disabled-text: 0;
}

QTabBar::tab:selected {
    color: white;
    border-color: #333333;
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                      stop:0 #67c2ee,
                      stop:0.7 #2986f5,
                      stop:1 #2986f5);
}

QTabBar::tab:selected:disabled {
    border-color: #999;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                    stop: 0 #cacaca, stop: 0.7 #9a9a9a,
                                    stop: 1 #9a9a9a);
    color: #eee;

}

QTabBar::tab:selected:hover {
}

QTabBar::tab:!selected {
    color: black;
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                      stop:0 white, stop:0.7 #cccccc, stop:1 #aaa);
    font-weight: normal;
}

QTabBar::tab:!selected:disabled {
    border-color: #999;
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                      stop:0 #fff,
                      stop:1 #cdcdcd);
    color: #999;
}

QTabBar::tab:!selected:hover {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                      stop:0 #e3e3e3, stop:0.6 #bbbbbb, stop:1 #888888);
}

