* {
    selection-color: white;
    selection-background-color: #318ef4;
    show-decoration-selected: 1;
    outline: none;
    font-family: "Open Sans";
}

*[optionalField="true"] {
    font-style: italic;
}

QLabel[flagLabel="true"] {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                  stop:0 white, stop:1 #D1E4E7);
    min-width: 100px;
    min-height: 17px;
    max-height: 17px;
    border-radius: 4px;
    border: 1px solid #666666;
}

QLabel[flagLabel="true"]:disabled {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                  stop:0 white, stop:1 #CCCCCC);
    min-width: 100px;
    min-height: 17px;
    max-height: 17px;
    border-radius: 4px;
    border: 1px solid #999;
}

QLineEdit {
    border: 1px solid #666666;
    border-radius: 4px;
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                      stop:0 white, stop:1 #dee6f1);
    color: #333333;
    font: normal normal 11px "Open Sans";
    max-height: 17px;
}

QLineEdit:disabled {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                      stop:0 #f3f3f4, stop:1 #e7e8e9);
    color: grey;
    border: 1px solid #999;
}

QLineEdit:hover
{
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                      stop:0 white, stop:1 #c6e1f4);
}

QLineEdit::focus {
    border: 1px outset;
    border-radius: 4px;
    /*border-color: rgb(41, 237, 215);
    color: rgb(0, 0, 0);
    background-color: rgb(150, 150, 150);*/
}

CustomLineEdit {
    border: 1px solid #666666;
    border-radius: 4px;
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                      stop:0 white, stop:1 #D1E4E7);
    color: #333333;
    font: normal normal 11px "Open Sans";
    max-height: 17px;
}

CustomLineEdit:disabled {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                      stop:0 #eff0f1, stop:1 #ddd);
    color: grey;
    border: 1px solid #999;
}

CustomLineEdit:hover
{
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                  stop:0 #e5f9ff, stop:0.25 #f4fcff,
                  stop:0.75 #d4f3ff, stop:1 #aae6ff);
}

CustomLineEdit[loadEdit="true"] {
    border: 1px solid #666666;
    border-top-width: 1px;
    border-right-width: 0px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-top-left-radius: 4px;
    border-top-right-radius: 0px;
    border-bottom-left-radius: 4px;
    border-bottom-right-radius: 0px;
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                      stop:0 white, stop:1 #D1E4E7);
    color: #333333;
    font: normal normal 11px "Open Sans";
    max-height: 17px;
}

CustomLineEdit[loadEdit="true"]:disabled {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                      stop:0 #eff0f1, stop:1 #ddd);
    color: grey;
    border: 1px solid #999;
    border-top-width: 1px;
    border-right-width: 0px;
    border-bottom-width: 1px;
    border-left-width: 1px;
}

CustomLineEdit[loadEdit="true"]:hover
{
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                  stop:0 #e5f9ff, stop:0.25 #f4fcff,
                  stop:0.75 #d4f3ff, stop:1 #aae6ff);
}

QSpinBox {
    padding-left: 4px;
    padding-right: 15px;
    border: 1px solid #666666;
    border-radius: 4px;
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                      stop:0 white, stop:1 #D1E4E7);
    color: #333333;
    font: normal normal 11px "Open Sans";
    max-height: 17px;
}

QSpinBox:disabled {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                      stop:0 white, stop:1 #CCCCCC);
    color: grey;
    border: 1px solid #999;
}

QSpinBox::up-button {
    subcontrol-origin: border;
    subcontrol-position: top right;
    width: 13px;
    height: 8px;
    border-image: url(:/icons/spinup) 1;
    border-width: 1px;
}

QSpinBox::up-button:hover {
    border-image: url(:/icons/spinup-hover) 1;
}

QSpinBox::up-button:pressed {
    border-image: url(:/icons/spinup-pressed) 1;
}

QSpinBox::up-button:disabled {
    border-image: url(:/icons/spinup-disabled) 1;
}

QSpinBox::up-arrow {
    image: url(:/icons/spinup-arrow);
    width: 5px;
    height: 3px;
}

QSpinBox::up-arrow:disabled, QSpinBox::up-arrow:off {
   image: url(:/icons/spinup-arrow-disabled);
}

QSpinBox::down-button {
    border-left-width: 1px;
    width: 13px;
    border-image: url(:/icons/spindown) 1;
}

QSpinBox::down-button:hover {
    border-image: url(:/icons/spindown-hover) 1;
}

QSpinBox::down-button:pressed {
    border-image: url(:/icons/spindown-pressed) 1;
}

QSpinBox::down-button:disabled {
    border-image: url(:/icons/spindown-disabled) 1;
}

QSpinBox::down-arrow {
    image: url(:/icons/spindown-arrow);
    width: 5px;
    height: 3px;
    margin-top: -1px;
}

QSpinBox::down-arrow:disabled,
QSpinBox::down-arrow:off {
   image: url(:/icons/spindown-arrow-disabled);
}

QDoubleSpinBox {
    padding-left: 4px;
    padding-right: 15px;
    border: 1px solid #666666;
    border-radius: 4px;
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                      stop:0 white, stop:1 #D1E4E7);
    color: #333333;
    font: normal normal 11px "Open Sans";
    max-height: 17px;
}

QDoubleSpinBox:disabled {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                      stop:0 white, stop:1 #CCCCCC);
    color: grey;
    border: 1px solid #999;
}

QDoubleSpinBox::up-button {
    subcontrol-origin: border;
    subcontrol-position: top right;
    width: 13px;
    height: 8px;
    border-width: 1px;
    border-image: url(:/icons/spinup) 1;
}

QDoubleSpinBox::up-button:hover {
    border-image: url(:/icons/spinup-hover) 1;
}

QDoubleSpinBox::up-button:pressed {
    border-image: url(:/icons/spinup-pressed) 1;
}

QDoubleSpinBox::up-button:disabled {
    border-image: url(:/icons/spinup-disabled) 1;
}

QDoubleSpinBox::up-arrow {
    image: url(:/icons/spinup-arrow);
    width: 5px;
    height: 3px;
}

QDoubleSpinBox::up-arrow:disabled, QSpinBox::up-arrow:off {
    image: url(:/icons/spinup-arrow-disabled);
}

QDoubleSpinBox::down-button {
    border-left-width: 1px;
    width: 13px;
    border-image: url(:/icons/spindown) 1;
}

QDoubleSpinBox::down-button:hover {
    border-image: url(:/icons/spindown-hover) 1;
}

QDoubleSpinBox::down-button:pressed {
    border-image: url(:/icons/spindown-pressed) 1;
}

QDoubleSpinBox::down-button:disabled {
    border-image: url(:/icons/spindown-disabled) 1;
}

QDoubleSpinBox::down-arrow {
    image: url(:/icons/spindown-arrow);
    width: 5px;
    height: 3px;
    margin-top: -1px;
}

QDoubleSpinBox::down-arrow:disabled,
QDoubleSpinBox::down-arrow:off {
   image: url(:/icons/spindown-arrow-disabled);
}

QComboBox {
    border: 1px solid #666666;
    border-radius: 4px;
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                      stop:0 white, stop:1 #D1E4E7);
    color: #333333;
    font: normal normal 11px "Open Sans";
    min-width: 6em;
    padding: 1px 3px 1px 3px;
    max-height: 17px;
}

QComboBox:disabled {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                      stop:0 white, stop:1 #CCCCCC);
    color: grey;
    border: 1px solid #999;
}

QComboBox:hover
{
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                  stop:0 #e5f9ff, stop:0.25 #f4fcff,
                  stop:0.75 #d4f3ff, stop:1 #aae6ff);
}

QComboBox:editable {
    font: normal normal 11px "Open Sans";
}

QComboBox:!editable, QComboBox::drop-down:editable {
    font: normal normal 11px "Open Sans";
}

QComboBox:!editable:on, QComboBox::drop-down:editable:on {
    font: normal normal 11px "Open Sans";
}

QComboBox:on {
    padding-top: 3px;
    padding-left: 4px;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 13px;
    border-left-width: 1px;
    border-left-color: #666666;
    border-left-style: solid;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
}

QComboBox::down-arrow {
    image: url(:/icons/combo-normal);
}

QComboBox::down-arrow:hover {
    left: 0px;
    image: url(:/icons/combo-hover);
}

QComboBox::down-arrow:on {
    left: 0px;
    image: url(:/icons/combo-selected);
}

QComboBox::down-arrow:disabled,
QComboBox::down-arrow:off {
    left: 0px;
    image: url(:/icons/combo-disabled);
}

MainWindow {
    background-color: white;
}

/* dock widget handle */
QMainWindow::separator {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                      stop:0 #e9e9e9, stop:0.5 #d9d9d9, stop:1 #d9d9d9);
    width: 5px;
    height: 5px;
}

QMainWindow::separator::vertical:left {
    background: qlineargradient(spread:pad, x1:0.993909, y1:0.455, x2:0, y2:0.511, stop:0 rgba(172, 172, 172, 255), stop:1 rgba(255, 255, 255, 255));
    width: 5px;
    height: 5px;
    border-radius: 3px;
    border-left: 1px solid black;
    border-right: 1px solid black;
}

QMainWindow::separator::vertical:right {
    background: qlineargradient(spread:pad, x1:0.993909, y1:0.455, x2:0, y2:0.511, stop:0 rgba(172, 172, 172, 255), stop:1 rgba(255, 255, 255, 255));
    width: 5px;
    height: 5px;
    border-radius: 3px;
    border-left: 1px solid black;
    border-right: 1px solid black;
}

QMainWindow::separator:hover::vertical {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 rgba(102, 102, 102, 255), stop:1 rgba(255, 255, 255, 255));
}

QMainWindow::separator::horizontal:top {
    background:qlineargradient(spread:pad, x1:0.494, y1:1, x2:0.483, y2:0.0166818, stop:0 rgba(172, 172, 172, 255), stop:1 rgba(255, 255, 255, 255));
    width: 5px;
    height: 5px;
    border-radius: 3px;
    border-top: 1px solid black;
    border-bottom: 1px solid black;
}

QMainWindow::separator::horizontal:bottom {
    background:qlineargradient(spread:pad, x1:0.494, y1:1, x2:0.483, y2:0.0166818, stop:0 rgba(172, 172, 172, 255), stop:1 rgba(255, 255, 255, 255));
    width: 5px;
    height: 5px;
    border-radius: 3px;
    border-top: 1px solid black;
    border-bottom: 1px solid black;
}

QMainWindow::separator:hover::horizontal{
    background: qlineargradient(spread:pad, x1:0.5, y1:0.017, x2:0.5, y2:1, stop:0 rgba(102, 102, 102, 255), stop:1 rgba(255, 255, 255, 255));
}

MainWidget {
    background-color: transparent;
}

QMenuBar {
    background-color: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.5, y2:1, stop:0 rgba(247, 247, 247, 255), stop:1 rgba(215, 215, 215, 255));
}

QMenuBar::item {
    background: transparent;
